<?xml version="1.0" encoding="UTF-8"?>
<TestCaseEntity>
   <description>Test Case: Cancel Deactivate User - Verifies that canceling deactivation keeps the user active</description>
   <name>TC_ADM_USER_006_CancelDeactivateUser</name>
   <tag>admin,users,deactivate,cancel</tag>
   <comment></comment>
   <testCaseGuid>tc-adm-user-006</testCaseGuid>
   <testCaseSteps>
      <TestCaseStep>
         <description>Click Deactivate button for specific user</description>
         <name>Click - Deactivate User Button</name>
         <testStepType>WebUI</testStepType>
         <keyword>click</keyword>
         <arguments>
            <argument>
               <name>to</name>
               <value>findTestObject(&quot;Object Repository/Admin/UsersPage/btn_DeactivateUser&quot;, [(&quot;userEmail&quot;): testUserEmail])</value>
            </argument>
         </arguments>
      </TestCaseStep>
      <TestCaseStep>
         <description>Wait for alert to appear</description>
         <name>Wait For Alert</name>
         <testStepType>WebUI</testStepType>
         <keyword>waitForAlert</keyword>
         <arguments>
            <argument>
               <name>timeout</name>
               <value>5</value>
            </argument>
         </arguments>
      </TestCaseStep>
      <TestCaseStep>
         <description>Dismiss the deactivation alert</description>
         <name>Dismiss Alert</name>
         <testStepType>Assertion</testStepType>
         <keyword>dismissAlert</keyword>
      </TestCaseStep>
   </testCaseSteps>
</TestCaseEntity>
