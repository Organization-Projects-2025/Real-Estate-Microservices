<?xml version="1.0" encoding="UTF-8"?>
<TestCaseEntity>
   <description>Test Case: Reactivate User - Verifies that admin can reactivate an inactive user</description>
   <name>TC_ADM_USER_007_ReactivateUser</name>
   <tag>admin,users,reactivate</tag>
   <comment></comment>
   <testCaseGuid>tc-adm-user-007</testCaseGuid>
   <testCaseSteps>
      <TestCaseStep>
         <description>Click Reactivate button for specific user</description>
         <name>Click - Reactivate User Button</name>
         <testStepType>WebUI</testStepType>
         <keyword>click</keyword>
         <arguments>
            <argument>
               <name>to</name>
               <value>findTestObject(&quot;Object Repository/Admin/UsersPage/btn_ReactivateUser&quot;, [(&quot;userEmail&quot;): testUserEmail])</value>
            </argument>
         </arguments>
      </TestCaseStep>
      <TestCaseStep>
         <description>Wait for alert to appear</description>
         <name>Wait For Alert</name>
         <testStepType>WebUI</testStepType>
         <keyword>waitForAlert</keyword>
         <arguments>
            <argument>
               <name>timeout</name>
               <value>5</value>
            </argument>
         </arguments>
      </TestCaseStep>
      <TestCaseStep>
         <description>Accept the reactivation alert</description>
         <name>Accept Alert</name>
         <testStepType>WebUI</testStepType>
         <keyword>acceptAlert</keyword>
      </TestCaseStep>
   </testCaseSteps>
</TestCaseEntity>
